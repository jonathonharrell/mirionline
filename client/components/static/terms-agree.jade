div
  .modal-header
    button.close(type='button', data-dismiss='modal', ng-click='$dismiss()')
      span(aria-hidden='true') &times;
      span.sr-only Close
    h3.modal-title Terms of Service
  .modal-body
    section(ng-include='"components/static/terms-content.html"')
  .modal-footer
    button.btn.btn-primary(type='button', ng-click='$close(true)') Agree and Create Account
    =' '
    button.btn.btn-default(type='button', ng-click='$dismiss()') Cancel
